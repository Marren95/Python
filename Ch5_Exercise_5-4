def is_triangle(a, b, c):
    if a <= b + c:
        if b <= a + c:
            if c <= a + b:
                print ("Yes")
    if a > b + c:
        print ("No")
    if b > a + c:
        print ("No")
    if c > a + b:
        print ("No")
    else:
        return

def int_input():
    a = int(input("What measurement would you like 'a' to be?\n"))
    b = int(input("'b'?\n"))
    c = int(input("'c'?\n"))
    is_triangle(a, b, c)

int_input()
